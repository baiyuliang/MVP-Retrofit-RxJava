<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Non focused states -->
    <item android:drawable="@mipmap/icon_tabbar_library" android:left="2dp" android:right="4dp" android:state_focused="false" android:state_pressed="false" android:state_selected="false" />
    <item android:drawable="@mipmap/icon_tabbar_library_selected" android:left="2dp" android:right="4dp" android:state_focused="false" android:state_pressed="false" android:state_selected="true" />
    <!-- Focused states -->
    <item android:drawable="@mipmap/icon_tabbar_library_selected" android:left="2dp" android:right="4dp" android:state_focused="true" android:state_pressed="false" android:state_selected="false" />
    <item android:drawable="@mipmap/icon_tabbar_library_selected" android:left="2dp" android:right="4dp" android:state_focused="true" android:state_pressed="false" android:state_selected="true" />
    <!-- Pressed -->
    <item android:drawable="@mipmap/icon_tabbar_library_selected" android:left="2dp" android:right="4dp" android:state_pressed="true" />
</selector>